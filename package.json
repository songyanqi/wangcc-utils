{
  "name": "wangcc-utils",
  "version": "1.3.1",
  "description": "wangchaochuan's util function library",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
